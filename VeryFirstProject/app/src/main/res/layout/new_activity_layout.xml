<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="ci.felight.ci.NewActivity">


    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Camera"
        android:id="@+id/btnCamera"
        android:layout_below="@+id/tvSET"
        android:layout_alignRight="@+id/tvSET"
        android:layout_alignEnd="@+id/tvSET" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Browser"
        android:id="@+id/btnBrowser"
        android:layout_below="@+id/btnCamera"
        android:layout_alignLeft="@+id/btnCamera"
        android:layout_alignStart="@+id/btnCamera" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="SMS"
        android:id="@+id/btnSms"
        android:layout_below="@+id/btnBrowser"
        android:layout_alignRight="@+id/btnBrowser"
        android:layout_alignEnd="@+id/btnBrowser" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Vedio"
        android:id="@+id/btnMusicPlayer"
        android:layout_below="@+id/btnSms"
        android:layout_alignRight="@+id/btnBrowser"
        android:layout_alignEnd="@+id/btnBrowser" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Call"
        android:id="@+id/btnCall"
        android:layout_below="@+id/btnMusicPlayer"
        android:layout_alignRight="@+id/btnBrowser"
        android:layout_alignEnd="@+id/btnBrowser" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Dialer"
        android:id="@+id/btnDialer"
        android:layout_below="@+id/btnCall"
        android:layout_alignRight="@+id/btnBrowser"
        android:layout_alignEnd="@+id/btnBrowser" />

    <Button
        android:onClick="newNavigator"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Toast"
        android:id="@+id/btnToast"
        android:layout_below="@+id/btnDialer"
        android:layout_alignLeft="@+id/btnDialer"
        android:layout_alignStart="@+id/btnDialer" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/tvContact"
        android:layout_alignParentBottom="true"
        android:layout_marginLeft="57dp"
        android:layout_marginStart="57dp"
        android:textSize="30dp"
        android:layout_alignLeft="@+id/btnToast"
        android:layout_alignStart="@+id/btnToast" />

</RelativeLayout>
